{
  "name": "bouqt-v2",
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "test": "playwright test",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:push": "prisma db push",
    "db:gen": "prisma generate",
    "db:seed": "tsx scripts/seed.ts",
    "db:reset": "npx prisma db push --force-reset && tsx scripts/seed.ts",
    "clean": "rimraf .next .turbo node_modules dist && npm install",
    "bump": "npm version patch --no-git-tag-version",
    "nukerepo": "rm -rf .git && git init --quiet && git remote add origin https://github.com/mattwoodco/bouqt-e2etest-travis && git checkout -b main && git add . && git commit -m 'initial' && git push --set-upstream origin main --force",
    "postinstall": "prisma generate --no-engine",
    "prepare": "husky"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.4.2",
    "@bytescale/sdk": "^3.44.0",
    "@bytescale/upload-widget-react": "^4.19.0",
    "@prisma/client": "^5.18.0",
    "@prisma/extension-accelerate": "^1.1.0",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@react-email/components": "0.0.22",
    "@sentry/nextjs": "^8.24.0",
    "bytescale": "^1.2.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "jsdom": "^24.1.1",
    "lucide-react": "^0.424.0",
    "mailslurp": "^0.0.1-security",
    "mailslurp-client": "^15.20.2",
    "moment": "^2.30.1",
    "next": "14.2.5",
    "next-auth": "^5.0.0-beta.20",
    "next-themes": "^0.3.0",
    "prisma": "^5.18.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-email": "2.1.6",
    "resend": "^3.5.0",
    "stripe": "^16.6.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@playwright/test": "^1.46.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "eslint": "8",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.4",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "rimraf": "^6.0.1",
    "tailwind-merge": "^2.4.0",
    "tailwindcss": "^3.4.8",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.16.5",
    "turbo": "^2.0.12",
    "typescript": "^5.5.4"
  },
  "description": "Audio marketplace",
  "author": "Matt Wood <matt@mattwood.co>",
  "private": true,
  "packageManager": "npm@10.8.2",
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  }
}
